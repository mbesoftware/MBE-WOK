{
    "name": "MBE-WOK",
    "description": "MBE-WOK. Whatsapp Otonom Kullanıcı Yazılımı",
    "logo": "",
    "keywords": ["whatsapp", "MBE"],
    "repository": "https://github.com/mbesoftware",
    "website":   "https://github.com/mbesoftware",
    "success_url": "https://github.com/mbesoftware",
    "stack": "container",
    "env": {
        "MBE_SESSION": {
            "description": "Qr.Js ile oluşturduğunuz özel session kodu.",
            "required": true
        },
        "SUDO": {
            "description": "Leave blank if you don't know how to use it! Sudo mode; Your number must be an international number. So you should have your number after the country code. Example: 1205xxxxxxxxx (USA number +1)",
            "required": false
        },
        "LANGUAGE": {
            "description": "MBE-WOK Dili. Azerbaycan Türkçesi => az, English => en, Türkçe => tr, Malayalam => ml, Hindi => HI, Indonesia => IN, Español => ES",
            "required": true,
            "value": "TR"
        },
        "HANDLERS": {
            "description": "Komutlar için.",
            "required": true,
            "value": "^[.!;]"
        },
        "SEND_READ": {
            "description": "Görüldü atmak istiyorsanız true yazın.",
            "required": true,
            "value": "true"
        },
        "HEROKU": {
            "description": "Herokuda çalışıyorsa true yazın.",
            "required": true,
            "value": "true"
        },
        "HEROKU_API_KEY": {
            "description": "Herokuda API Keyiniz. Heroku API Key.",
            "required": true
        },
        "HEROKU_APP_NAME": {
            "description": "Herokuda uygulama adı. Heroku APP Name.",
            "required": true
        },
        "DEBUG": {
            "description": "Geliştiricilere özeldir.",
            "required": true,
            "value": "false"
        },
        "REMOVE_BG_API_KEY": {
            "description": "Fotoğrafların arkaplanını kaldırır. https://remove.bg",
            "required": false
        },
        "NO_ONLINE": {
            "description": "Çevrimiçi görünmek istemiyorsanız true yazın.",
            "required": false,
            "value": "true"
        },
        "COFFEEHOUSE_API_KEY": {
            "description": "CoffeeHouse API Key. Need for Lydia. https://coffeehouse.intellivoid.net/",
            "required": false,
            "value": "true"
        }
    },
    "addons": [{
      "plan": "heroku-postgresql"
    }],
    "buildpacks": [{
      "url": "heroku-community/apt"
      }]
  }
